library Init {
    real y = 0;
    integer index = - 1;
    framehandle fa, fb, gameUi;

    function onInit() {
        trigger t = CreateTrigger();
        TriggerRegisterPlayerEvent(t, Player(0), EVENT_PLAYER_ARROW_LEFT_DOWN);
        TriggerRegisterPlayerEvent(t, Player(0), EVENT_PLAYER_ARROW_RIGHT_DOWN);
        TriggerAddAction(t, function () {
            if (GetTriggerPlayerKey() == OSKEY_LEFT){
                BJDebugMsg("left");
                return;
            }
            if (GetTriggerPlayerKey() == OSKEY_RIGHT) {
                BJDebugMsg("right");
                return;
            }
            BJDebugMsg("fuckup");
        });
    }
}